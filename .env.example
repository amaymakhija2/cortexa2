# Authentication credentials (set in Cloudflare Pages dashboard)
VITE_AUTH_USERNAME=your_username
VITE_AUTH_PASSWORD=your_password

# Token signing secret (MUST match AUTH_SECRET on server)
# Used to generate API tokens on login
VITE_AUTH_SECRET=your_auth_secret

# Gemini API Key (optional - for AI features)
GEMINI_API_KEY=your_gemini_api_key

# API Security (set in Cloudflare Pages dashboard)
# AUTH_SECRET must match VITE_AUTH_SECRET above
AUTH_SECRET=your_auth_secret
ALLOWED_ORIGIN=https://your-domain.pages.dev
